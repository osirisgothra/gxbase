TRUENAME=`readlink -e $BASH_SOURCE`
TRUEDIR=`dirname $TRUENAME`
if "$TRUEDIR/goo.pl" "$@";  then
	echo "Completed Script : No Errors Reported"
else
	echo "Error $? reported, ensure you have perl, and try again!"
	echo "Also, make sure you have the perl modules required by gxbase."
fi


