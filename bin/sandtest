#!/bin/bash

#sandbox

case $1 in

	"")
	echo "Nothing";;
	
	UNKNOWN)
	echo "Is unknown but...";;&
	# what does this do?
	
	UNKNOWNAGAIN)
	echo "Is unknown, again, but...";;
	# and is the only thing to get parsed!!!
	
	ENKN)
	echo "Is this...";&
	
	DEPRESSED)
	echo "Is Depressed";&
	# note: when you are depressed you are also sad so...
	SAD)
	echo "Is SAD";;
	
	*KN*)
	echo "This is KN (before star)";;
	
	*NK*)
	echo "This is NK (after star)";;

esac
	
	
	
