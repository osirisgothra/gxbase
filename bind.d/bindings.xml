<?xml version="1.0" lang="en_US"?>
<gxbase kind="keybindings" xml:space="preserve">
	<definitions>
		<key name="F1">
			<keyvalue terminal="linux">[[A</keyvalue>
			<keyvalue terminal="*">OP</keyvalue>
		</key>
		<key name="ESC">
			<keyvalue terminal="*"></keyvalue>
		</key>
	<definitions>

	<bindings>
		<binding name="HelpKeys" summary="Show Bound Key Sequences" function="help_keys">
			<keysequence>
				<key>F1</key>
				<key>ESC</key>
			</keysequence>
		</binding>
	</bindings>

</gxbase>
<?gxbase processor="$GXBASE_ROOT/bind.d/helpkeys.sh"?>


