
#!/bin/bash
#
# bash-settings
#
# bash-specific settings
#
# u (shopt), or the + to - (set) to change settings
# 
# updates
#
# 5/31/2014 - standardizing all script activity to use set -u
#             but ONLY on debug versions
#
#DNMBTL DO NOT MODIFY BEYOND THIS LINE:  REST IS GENERATED BY THE SHOPTEDITOR

	# Shell Configuration
	# usually disabled                             usually enabled
																																											
	shopt -u autocd                               ;shopt -s checkwinsize                 
	shopt -u cdable_vars                          ;shopt -s cmdhist                      
	shopt -u cdspell															;shopt -s complete_fullquote           
	shopt -u checkhash                            ;shopt -s direxpand                    
	shopt -u checkjobs                            ;shopt -s expand_aliases               
	shopt -u compat31                             ;shopt -s extdebug                     
	shopt -u compat32                             ;shopt -s extglob                      
	shopt -u compat40                             ;shopt -s extquote                     
	shopt -u compat41                             ;shopt -s force_fignore                
	shopt -u compat42                             ;shopt -s globstar                     
	shopt -u dirspell                             #;shopt -s histappend                   
	shopt -u dotglob                              #;shopt -s histreedit                   
	shopt -u execfail                             ;shopt -s interactive_comments         
	shopt -u globasciiranges                      ;shopt -s login_shell                  
	shopt -u gnu_errfmt                           ;shopt -s mailwarn                     
	shopt -u failglob                             ;shopt -s progcomp                     
	shopt -u histverify                           ;shopt -s promptvars                   
	shopt -u hostcomplete                         ;shopt -s sourcepath                   
	shopt -u huponexit                            ;shopt -s xpg_echo                     
	shopt -u lastpipe                            
	shopt -u lithist              
	shopt -u no_empty_cmd_completion              
	shopt -u nocaseglob           
	shopt -u nocasematch                          
	shopt -u nullglob             
	shopt -u restricted_shell                     
	shopt -u shift_verbose        

	# disabled shell flags                        # enabled shell flags
																																										
	set +o allexport                              ;set -o braceexpand                 
	set +o errexit                                ;set -o emacs                       
	set +o errtrace                               ;set -o functrace                   
	set +o ignoreeof                              ;set -o hashall                     
	set +o keyword                                #;set -o histexpand                  
	set +o noclobber                              #;set -o history                     
	set +o noexec                                 ;set -o interactive-comments        
	set +o noglob                                 ;set -o monitor                     
	set +o nolog                                  
	set +o notify                 
	set +o nounset                                
	set +o onecmd                 
	set +o physical                               
	set +o pipefail               
	set +o posix                                  
	set +o privileged             
	set +o verbose                                
	set +o vi                     
	set +o xtrace

	#GXBASE CNF SETTINGS
	# ( generated from pass grep )
	
	#GXCNF_ prefixes all variables and is not included in docs to save my fingers from falling off

	#LSFUNC: prefix  suffix  meaning
	#                  =1    use enhanced (interactive-only) version of ls that outputs details, instead of ls. 
	#                  =2    use ls padded by $LS_HEAD/$LS_TAIL or spacelines if not defined
	#        unset           use normal ls command
	unset GXCNF_LSFUNC




	#GXBASE DEBUG OVERRIDES FOR SHELL OPTIONS
  # see above

	[[ $GXCNF_DEBUG_OVERRIDES ]] && {


	#GXBASE_ROOT= you can override it here, if you are caching to memory, for example
	GXBASE_ROOT=$GXBASE_ROOT
#bash-aliases:GXBASE_RES_BASHALIASES
#bash-aliases:GXBASE_ROOT
#bash-aliases:GXCNF_LSFUNC
#bash-aliases:GXCNF_NANOALT
#bash-aliases:GXCNF_NOCOLOR
#bash-aliases:GXCNF_NOGXDEVALIASES
#bash-aliases:GXCNF_NOHREXTRAS
#bash-aliases:GXCNF_NOLSEXTRAS
#bash-aliases:GXCNF_STARTX_IPS
#bash-aliases:GXCNF_VIMFUNC
#bashrc:GXBASE_LOADING
#bashrc:GXBASE_UUID
#bash-vars:GXBASE_RES_BASHVARS
#bash-vars:GXBASE_ROOT
#complete:GXBASE_COMPONENTS
#complete:GXBASE_LOADING
#complete:GXBASE_LOG
#config:GXBASE_COMPONENTS
#config:GXBASE_LOADING
#core:GXBASE_COMPONENTS
#core:GXBASE_LOADING
#core:GXBASE_ROOT
#envinfo:GXBASE_COMPONENTS
#envinfo:GXBASE_LOADING
#/gxbase/bin/extras/addgensudo:GXBASE_ROOT
#/gxbase/bin/extras/autovim:GXBASE_ROOT
#/gxbase/bin/extras/gen-bg:GNAME
#/gxbase/bin/extras/gen-sudo:GNAME
#/gxbase/bin/extras/goo:GBROWSER
#/gxbase/bin/extras/gxbase-legacy-environment:GEM_OPTIONS
#/gxbase/bin/extras/gxbase-legacy-environment:GENERATE_INDEX_OPTIONS
#/gxbase/bin/extras/gxbase-legacy-environment:GIT_DIR
#/gxbase/bin/extras/gxbase-legacy-environment:GIT_PS
#/gxbase/bin/extras/gxbase-legacy-environment:GPG_AGENT_INFO
#/gxbase/bin/extras/gxbase-legacy-environment:GROUPS
#/gxbase/bin/extras/gxbase-legacy-environment:GS_LIB
#/gxbase/bin/extras/gxbase-legacy-environment:GXBASE_EXITBANNER_DISPLAYTIME
#/gxbase/bin/extras/gxbase-legacy-environment:GXBASE_INIT
#/gxbase/bin/extras/gxbase-legacy-environment:GXBASE_PRIMARY
#/gxbase/bin/extras/gxbase-legacy-environment:GXBASE_RETURN
#/gxbase/bin/extras/gxbase-legacy-environment:GXBASE_ROOT
#/gxbase/bin/extras/gxbase-legacy-environment:GXBASE_STARTUP_VERBOSITY_LEVEL
#/gxbase/bin/extras/gxbase-legacy-environment:GXBASE_VARS
#/gxbase/bin/extras/gxbase-legacy-environment:GXBASE_VARSCLR
#/gxbase/bin/extras/gxbase-legacy-environment:GXBASE_VARSDYN
#/gxbase/bin/extras/gxbase-legacy-environment:GXBASE_VARSSET
#/gxbase/bin/extras/gxbase-legacy-environment:GXBASE_VARSXML
#/gxbase/bin/extras/kate:GNAME
#/gxbase/bin/extras/kezendiatail:GXBASE_ROOT
#/gxbase/bin/extras/readme-bin-dir.txt:GXBASE_ROOT
#/gxbase/bin/extras/vidmode:GXBASE_ROOT
#template-component:GXBASE_COMPONENTS
#template-component:GXBASE_LOADING
#ui:GXBASE_COMPONENTS
#ui:GXBASE_LOADING

}

	


